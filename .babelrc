{
    "presets": ["@babel/preset-env"],
    "plugins": [
        "@vue/babel-plugin-jsx",
        "@babel/plugin-transform-runtime",
        "@babel/plugin-syntax-dynamic-import",
        [
            "@babel/plugin-transform-typescript",
            {
              "isTSX": true,
              "allowNamespace": true
            }
        ]
    ]
}